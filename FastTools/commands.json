[
  {
    "alias": "root & remount",
    "steps": [
      { "type": "adb_command", "value": "adb -s {dev} root" },
      { "type": "delay", "value": "500" },
      { "type": "adb_command", "value": "adb -s {dev} wait-for-device" },
      { "type": "adb_command", "value": "adb -s {dev} remount" },
      { "type": "adb_command", "value": "adb -s {dev} shell setenforce 0" }
    ]
  },
  {
    "alias": "kfc",
    "steps": [
      { "type": "adb_command", "value": "adb -s {dev} shell df -h" }
    ]
  },
