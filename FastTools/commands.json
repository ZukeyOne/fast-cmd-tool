[
  {
    "alias": "root & remount",
    "steps": [
      { "type": "adb_command", "value": "adb -s {dev} root" },
      { "type": "delay", "value": "1000" },
      { "type": "adb_command", "value": "adb -s {dev} remount" }
    ]
  },
  {
    "alias": "df -h",
    "steps": [
      { "type": "adb_command", "value": "adb -s {dev} shell df -h" }
    ]
  },
  {
    "alias": "ls /sdcard",
    "steps": [
      { "type": "adb_command", "value": "adb -s {dev} shell ls /sdcard" }
    ]
  }
]
